[MESSAGES CONTROL]

disable=
  line-too-long,
  multiple-imports,
  fixme,
  missing-function-docstring,
  multiple-statements,
  too-many-locals,
  trailing-comma-tuple,
  no-else-return,
  missing-class-docstring,
  no-else-raise,
  too-few-public-methods,
  global-statement,
  too-many-instance-attributes,

[FORMAT]

indent-string = "  "
indent-after-paren=2
